{
  "name": "whitelabel-messagebox-certifier",
  "version": "1.0.0",
  "description": "Whitelabel MessageBox certifier and lookup platform",
  "main": "server/dist/index.js",
  "scripts": {
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "cd frontend-next && npm run dev",
    "build:server": "cd server && npm run build",
    "build:frontend": "cd frontend-next && npm run build",
    "start:server": "cd server && npm start",
    "start:frontend": "cd frontend-next && npm start",
